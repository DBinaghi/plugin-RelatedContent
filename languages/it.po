msgid "Criterion"
msgstr "Criterio"

#: config_form.php:68
msgid "Weight"
msgstr "Peso"

#: config_form.php:69
msgid "Constraint"
msgstr "Vincolo"

#: config_form.php:70
msgid "Is Date"
msgstr "È data"

#: config_form.php:119
msgid "Other criteria"
msgstr "Altri criteri"

#: config_form.php:123
msgid "Item Type"
msgstr "Tipologia documento"

